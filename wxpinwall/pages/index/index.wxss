/* 作品详情弹出层 */
.popup{
    display: flex;
    flex-direction: column;
}
.popup .artifical_detail{
    display: flex;
    flex-direction: column;
    width: 700rpx;
    height: 480rpx;
    margin:0px auto 110rpx;
    background: rgba(225, 225, 225, 0.9);
    border-radius: 24rpx;
}
.popup .artifical_detail .userAvator{
    width: 100rpx;
    height: 100rpx;
    position: absolute;
    margin-top: 35rpx;
    margin-left: 60rpx;
    border-radius: 70rpx;
    z-index: 1;
}
.popup .artifical_detail .username{
    width: 350rpx;
    height: 40rpx;
    position: absolute;
    margin-top: 35rpx;
    margin-left: 180rpx;
    font-size: 30rpx;
    z-index: 1;
}
.popup .artifical_detail .createAt{
    width: 250rpx;
    height: 40rpx;
    position: absolute;
    margin-top: 90rpx;
    margin-left: 180rpx;
    color:#999999;
    font-size: 26rpx;
    opacity: 1;
    z-index: 1;
}
.popup .artifical_detail .title{
    width: 600rpx;
    height: 60rpx;
    line-height: 60rpx;
    position: absolute;
    margin-top: 140rpx;
    margin-left: 60rpx;
    color:#1FB5BE;
    font-size: 32rpx;
    font-weight: bold;
    opacity: 1;
    display: -webkit-box;
    word-break: break-all;
    -webkit-box-orient: vertical;
    -webkit-line-clamp:1;
    overflow: hidden;
    text-overflow:ellipsis;
    z-index: 1;
}
.popup .artifical_detail .descript{
    width: 600rpx;
    height: 95rpx;
    line-height: 32rpx;
    position: absolute;
    margin-top: 210rpx;
    margin-left: 60rpx;
    color:#999999;
    font-size: 24rpx;
    opacity: 1;
    z-index: 1;

    display: -webkit-box;
    word-break: break-all;
    -webkit-box-orient: vertical;
    -webkit-line-clamp:3;
    overflow: hidden;
    text-overflow:ellipsis;
}
.popup .artifical_detail .shoulu{
    width: 600rpx;
    height: 40rpx;
    line-height: 32rpx;
    position: absolute;
    margin-top: 320rpx;
    margin-left: 60rpx;
    color:#333333;
    font-size: 24rpx;
    opacity: 1;
    z-index: 1;
}
.popup .artifical_detail .topic{
    width: 600rpx;
    height: 50rpx;
    line-height: 50rpx;
    position: absolute;
    margin-top: 360rpx;
    margin-left: 60rpx;
    color:rgba(51, 51, 51, 1);
    font-size: 24rpx;
    font-weight: bold;
    display: -webkit-box;
    word-break: break-all;
    -webkit-box-orient: vertical;
    -webkit-line-clamp:1;
    overflow: hidden;
    text-overflow:ellipsis;
}
.popup .artifical_detail .medal{
    width: 30rpx;
    height: 46rpx;
    position: absolute;
    margin-top: 420rpx;
    margin-left: 60rpx;
}
.popup .artifical_detail .medalCount{
    width: 30rpx;
    height: 30rpx;
    line-height: 30rpx;
    position: absolute;
    margin-top: 430rpx;
    margin-left: 100rpx;
    color:rgba(151, 151, 151, 1);
    font-size: 24rpx;
}
.popup .artifical_detail .like{
    width: 42rpx;
    height: 42rpx;
    position: absolute;
    margin-top: 420rpx;
    margin-left: 194rpx;
}
.popup .artifical_detail .likeCount{
    width: 30rpx;
    height: 30rpx;
    line-height: 30rpx;
    position: absolute;
    margin-top: 430rpx;
    margin-left: 250rpx;
    color:rgba(151, 151, 151, 1);
    font-size: 24rpx;
}
.popup .artifical_detail .comment{
    width: 42rpx;
    height: 38rpx;
    position: absolute;
    margin-top: 424rpx;
    margin-left: 338rpx;
}
.popup .artifical_detail .commentCount{
    width: 30rpx;
    height: 30rpx;
    line-height: 30rpx;
    position: absolute;
    margin-top: 430rpx;
    margin-left: 390rpx;
    color:rgba(151, 151, 151, 1);
    font-size: 24rpx;
}

.popup .unsubmit{
    width:700rpx;
    height:100rpx;
    position: absolute;
    bottom: 20rpx;
    margin: 0 auto;
}
.popup .unsubmit button{
    width: 700rpx;
    height:90rpx;
    line-height:90rpx;
    font-size: 40rpx;
    margin-left: 0px;
    border-radius: 30rpx;
    color:#1FB5BE !important;
}
