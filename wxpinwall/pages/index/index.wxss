.container{
    background: #fff;
    width: 700rpx;
    padding: 25rpx;
    display: flex;
    flex-direction: column;
}
.container .row_1,
.container .row_2{
    display: flex;
    flex-direction: row;
}
.container .row_1 .column_1,
.container .row_2 .column_2{
    display: flex;
    flex-direction: column
}
.container .row_1 .row_1_1,
.container .row_1 .row_1_2,
.container .row_2 .row_2_2,
.container .row_2 .row_2_3{
    margin: 5rpx;
    width: 225rpx;
    height: 225rpx;
}
.container .row_1 .row_1_1 image,
.container .row_1 .row_1_2 image,
.container .row_2 .row_2_2 image,
.container .row_2 .row_2_3 image{
    border-radius: 15rpx;
    width: 225rpx;
    height: 225rpx;
}
.container .row_1 .row_1_1 .point,
.container .row_1 .row_1_2 .point,
.container .row_2 .row_2_2 .point,
.container .row_2 .row_2_3 .point{
    position: absolute;
    margin-top: -52rpx;
    margin-left: 145rpx;
    width: 80rpx;
    height: 40rpx;
    line-height: 40rpx;
    font-weight: bold;
    font-size: 16px;
    text-align: center;
    color: #fff;
    background: rgba(0, 0, 0, 0.44);
    border-radius: 5rpx;
    border-bottom-right-radius: 15rpx;
}
.container .row_1 .row_1_3,
.container .row_2 .row_2_1{
